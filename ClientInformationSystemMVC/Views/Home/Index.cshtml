@model ApplicationCore.Models.InteractionPageViewModel

@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Employees</h1>
    <ul class="list-group list-group-flush">
        @foreach (var employee in Model.header.employeeDropDown)
        {
            <li class="list-group-item">

                <a href="http://localhost:44300/interaction" class="btn btn-primary btn-lg active" role="button" aria-pressed="true">@employee.Name</a>
            </li>
        }
    </ul>
    <h1 class="display-4">Clients</h1>
    <ul class="list-group list-group-flush">
        @foreach (var client in Model.header.clientDropDown)
        {
            <li class="list-group-item">

                <a href="http://localhost:44300/interaction" class="btn btn-primary btn-lg active" role="button" aria-pressed="true">@client.Name</a>
            </li>
        }
    </ul>


</div>

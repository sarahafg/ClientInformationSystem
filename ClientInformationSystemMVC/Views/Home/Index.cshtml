@model List<ApplicationCore.Models.EmployeeResponseModel>

@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Employees</h1>
    <ul class="list-group list-group-flush">
        @foreach (var client in Model)
        {
            <li class="list-group-item">

                <a href="http://localhost:44300/employee" class="btn btn-primary btn-lg active" role="button" aria-pressed="true">@client.Name</a>
            </li>
        }
    </ul>

    
</div>
